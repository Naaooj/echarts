{
  "name": "echarts",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --port 9090 --env.dev -d --watch",
    "serve:prod": "webpack-dev-server --env.prod",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "clean-dist": "rimraf dev && rimraf ../dist",
    "copy-files": "copyfiles -f 'src/index.html' './dist'",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build:prod": "webpack --config config/webpack.prod.js --env.prod"
  },
  "repository": "https://github.com/Naaooj/echarts",
  "author": "Johann Bernez",
  "license": "",
  "dependencies": {
    "echarts": "^3.7.2",
    "ngx-echarts": "^1.2.4",
    "@angular/common": "~4.2.6",
    "@angular/compiler": "~4.2.6",
    "@angular/core": "~4.2.6",
    "@angular/forms": "~4.2.6",
    "@angular/http": "~4.2.6",
    "@angular/platform-browser": "~4.2.6",
    "@angular/platform-browser-dynamic": "~4.2.6",
    "@angular/router": "~4.2.6",
    "@types/node": "^7.0.32",
    "core-js": "^2.4.1",
    "rxjs": "5.4.0",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
